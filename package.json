  "main": "dist/index.js",
  "types": "src/types.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/HadaIonut/dice-parser"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
